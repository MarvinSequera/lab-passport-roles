<h1>Lista De Empleados</h1>
<ul>
  {{#each users}}
  <li>Nombre del usuario {{this.username}} y tiene un role de {{this.role}}
    {{/each}}
</ul>
<form method="POST">
  <h2>Modifica tu Profile</h2>
  <div class="form-group">
    <label for="exampleInputEmail1">Usuario</label>
    <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" value={{user.username}}
      name="username">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Password</label>
    <input type="password" class="form-control" id="exampleInputPassword1" placeholder="New Password" name="password">
  </div>
  <div class="form-group">
    <p>Tu Role es de {{user.role}}</p>
  </div>
  <button type="submit" class="btn btn-primary">Registrar</button>
</form>